<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/goodsback">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:ignore="UselessParent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#00FFFFFF"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/button"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_weight="1"
                        android:background="#EAB4F3"
                        android:fontFamily="@font/bmjua"
                        android:text="@string/an"
                        android:textSize="24sp"
                        tools:ignore="ButtonStyle" />

                    <Button
                        android:id="@+id/button2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|center"
                        android:layout_weight="1"
                        android:background="#F1D0F6"
                        android:fontFamily="@font/bmjua"
                        android:text="@string/doll"
                        android:textSize="24sp"
                        tools:ignore="ButtonStyle" />

                    <Button
                        android:id="@+id/button3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|right"
                        android:layout_weight="1"
                        android:background="#EAB4F3"
                        android:fontFamily="@font/bmjua"
                        android:text="@string/mr"
                        android:textSize="24sp"
                        tools:ignore="ButtonStyle,RtlHardcoded" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#00F3FD8E"
                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    tools:ignore="NestedWeights">

                    <ImageView
                        android:id="@+id/imageView3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#9DD7F1"
                        android:padding="40dp"
                        app:srcCompat="@drawable/sujeong"
                        android:contentDescription="@string/sujeong" />

                    <ImageView
                        android:id="@+id/imageView2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#9DD7F1"
                        app:srcCompat="@drawable/doll"
                        android:contentDescription="@string/doll_doll" />

                    <ImageView
                        android:id="@+id/imageView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#9DD7F1"
                        app:srcCompat="@drawable/andae"
                        android:contentDescription="@string/andae" />

                    <LinearLayout
                        android:id="@+id/mr"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#9DD7F1"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageView6"
                            android:layout_width="200dp"
                            android:layout_height="match_parent"
                            android:padding="30dp"
                            app:srcCompat="@drawable/sujeong"
                            android:contentDescription="@string/sujeong_image" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView8"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:fontFamily="@font/bmjua"
                                android:gravity="center"
                                android:text="@string/su_mr"
                                android:textSize="24sp" />

                            <TextView
                                android:id="@+id/textView9"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/bmjua"
                                android:text="@string/shop_text"
                                android:textSize="18sp" />

                            <Button
                                android:id="@+id/buy3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/bmjua"
                                android:text="@string/buy"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/textView10"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/bmjua"
                                android:gravity="center"
                                android:text="@string/coin_mi"
                                android:textSize="16sp" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/dll"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#9DD7F1"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageView5"
                            android:layout_width="200dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0"
                            app:srcCompat="@drawable/doll"
                            android:contentDescription="@string/doll_doll_doll" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="0"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView5"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:fontFamily="@font/bmjua"
                                android:gravity="center"
                                android:text="@string/doll2"
                                android:textSize="24sp" />

                            <TextView
                                android:id="@+id/textView6"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/bmjua"
                                android:text="@string/doll_info"
                                android:textSize="18sp" />

                            <Button
                                android:id="@+id/buy2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/bmjua"
                                android:text="@string/buy"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/textView7"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/bmjua"
                                android:gravity="center"
                                android:text="@string/coin_mi4"
                                android:textSize="16sp" />
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ad"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#9DD7F1"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageView4"
                            android:layout_width="200dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0"
                            app:srcCompat="@drawable/andae"
                            android:contentDescription="@string/andae_andae" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="0"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:fontFamily="@font/bmjua"
                                android:gravity="center"
                                android:text="@string/andae2"
                                android:textSize="24sp" />

                            <TextView
                                android:id="@+id/textView3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/bmjua"
                                android:text="@string/andae_info"
                                android:textSize="18sp" />

                            <Button
                                android:id="@+id/buy1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/bmjua"
                                android:text="@string/buy"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/textView4"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/bmjua"
                                android:gravity="center"
                                android:text="@string/coin_mi2"
                                android:textSize="16sp" />
                        </LinearLayout>
                    </LinearLayout>

                </FrameLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#00FFFFFF"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="50dp"
                    android:fontFamily="@font/bmjua"
                    android:gravity="center_horizontal"
                    android:text="@string/welcome_shop"
                    android:textSize="20sp" />

            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="10"
            tools:ignore="UselessParent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:orientation="horizontal"
                android:weightSum="10">

                <Button
                    android:id="@+id/button175"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/imageView29"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="8"
                    android:adjustViewBounds="true"
                    app:srcCompat="@drawable/inven"
                    android:contentDescription="@string/inven" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_weight="7.8"
                android:layout_marginHorizontal="10dp">

                <TableLayout
                    android:id="@+id/table9"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="4"
                        android:background="#555555">

                        <Button
                            android:id="@+id/button176"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle" />

                        <Button
                            android:id="@+id/button177"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle" />

                        <Button
                            android:id="@+id/button178"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle" />

                        <Button
                            android:id="@+id/button179"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle" />

                        <Button
                            android:id="@+id/button180"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginLeft="7dp"
                            android:layout_marginRight="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle" />
                    </TableRow>

                </TableLayout>

            </LinearLayout>

            <Button
                android:id="@+id/button181"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="2.2"
                android:visibility="invisible" />

        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/button182"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/out"
            android:background="@drawable/button_radius2"
            android:textColor="#000000"
            android:fontFamily="@font/bmjua"
            android:layout_margin="10dp"
            app:autoSizeMaxTextSize="24dp"
            app:autoSizeMinTextSize="12dp"
            app:autoSizeStepGranularity="2dp"
            app:autoSizeTextType="uniform"
            android:layout_marginHorizontal="5dp" />
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>