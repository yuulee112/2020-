<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/button5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/hill"
            tools:visibility="visible" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/forus"
            android:contentDescription="@string/todo" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="5"
            tools:ignore="UselessParent">

            <Button
                android:id="@+id/button6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:visibility="invisible"
                tools:visibility="invisible" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1"
                android:weightSum="5">

                <Button
                    android:id="@+id/button13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:visibility="invisible"
                    tools:visibility="invisible"
                    tools:ignore="NestedWeights" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:layout_weight="1">

                    <Button
                        android:id="@+id/button7"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/bmjua"
                        android:textColor="#000000"
                        android:background="@drawable/button_selector"
                        android:text="@string/sujeong_build"
                        app:autoSizeMaxTextSize="30dp"
                        app:autoSizeMinTextSize="12dp"
                        app:autoSizeStepGranularity="2dp"
                        app:autoSizeTextType="uniform"/>

                    <Button
                        android:id="@+id/button8"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/bmjua"
                        android:textColor="#000000"
                        android:background="@drawable/button_selector"
                        android:text="@string/sungshin_build"
                        android:layout_marginTop="10dp"
                        app:autoSizeMaxTextSize="30dp"
                        app:autoSizeMinTextSize="12dp"
                        app:autoSizeStepGranularity="2dp"
                        app:autoSizeTextType="uniform"/>

                    <Button
                        android:id="@+id/button10"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/bmjua"
                        android:textColor="#000000"
                        android:background="@drawable/button_selector"
                        android:text="@string/center_library"
                        android:layout_marginTop="10dp"
                        app:autoSizeMaxTextSize="30dp"
                        app:autoSizeMinTextSize="12dp"
                        app:autoSizeStepGranularity="2dp"
                        app:autoSizeTextType="uniform"/>

                    <Button
                        android:id="@+id/button11"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/bmjua"
                        android:textColor="#000000"
                        android:background="@drawable/button_selector"
                        android:text="@string/NanHyang"
                        android:layout_marginTop="10dp"
                        app:autoSizeMaxTextSize="30dp"
                        app:autoSizeMinTextSize="12dp"
                        app:autoSizeStepGranularity="2dp"
                        app:autoSizeTextType="uniform"/>

                    <Button
                        android:id="@+id/button12"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/bmjua"
                        android:textColor="#000000"
                        android:background="@drawable/button_selector"
                        android:text="@string/drawbuilding"
                        android:layout_marginTop="10dp"
                        app:autoSizeMaxTextSize="30dp"
                        app:autoSizeMinTextSize="12dp"
                        app:autoSizeStepGranularity="2dp"
                        app:autoSizeTextType="uniform"/>

                    <Button
                        android:id="@+id/button14"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/bmjua"
                        android:textColor="#000000"
                        android:background="@drawable/button_selector"
                        android:text="@string/musicbuilding"
                        android:layout_marginTop="10dp"
                        app:autoSizeMaxTextSize="30dp"
                        app:autoSizeMinTextSize="12dp"
                        app:autoSizeStepGranularity="2dp"
                        app:autoSizeTextType="uniform"/>

                    <Button
                        android:id="@+id/button15"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/bmjua"
                        android:textColor="#000000"
                        android:background="@drawable/button_selector"
                        android:text="@string/gym"
                        android:layout_marginTop="10dp"
                        app:autoSizeMaxTextSize="30dp"
                        app:autoSizeMinTextSize="12dp"
                        app:autoSizeStepGranularity="2dp"
                        app:autoSizeTextType="uniform"/>

                </LinearLayout>
            </LinearLayout>
            <TextView
                android:id="@+id/textView1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_weight="1"
                android:background="@drawable/talk"
                android:fontFamily="@font/bmjua"
                android:paddingLeft="20dp"
                android:paddingTop="15dp"
                android:paddingRight="20dp"
                android:text="@string/forus_hello"
                android:textColor="#000000"
                android:visibility="invisible"
                app:autoSizeMaxTextSize="28dp"
                app:autoSizeMinTextSize="12dp"
                app:autoSizeStepGranularity="2dp"
                app:autoSizeTextType="uniform" />

        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="10"
            tools:ignore="UselessParent">

            <Button
                android:id="@+id/button16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="9"
                android:visibility="invisible"
                tools:visibility="invisible" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_weight="1"
                android:background="@drawable/talk"
                android:fontFamily="@font/bmjua"
                android:paddingLeft="20dp"
                android:paddingTop="15dp"
                android:paddingRight="20dp"
                android:text="@string/forus_hello2"
                android:textColor="#000000"
                app:autoSizeMaxTextSize="28dp"
                app:autoSizeMinTextSize="12dp"
                app:autoSizeStepGranularity="2dp"
                app:autoSizeTextType="uniform" />
        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:weightSum="2"
            android:layout_margin="5dp"
            tools:ignore="UselessParent">

            <Button
                android:id="@+id/button18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_weight="1"
                android:visibility="invisible"
                tools:visibility="invisible"
                tools:ignore="ButtonStyle" />

            <Button
                android:id="@+id/button23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="@string/move"
                android:textColor="#000000"
                android:fontFamily="@font/bmjua"
                android:background="@drawable/button_radius"
                android:layout_weight="1"
                app:autoSizeMaxTextSize="24dp"
                app:autoSizeMinTextSize="12dp"
                app:autoSizeStepGranularity="2dp"
                app:autoSizeTextType="uniform"
                tools:ignore="ButtonStyle" />

        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:weightSum="6"
            tools:ignore="UselessParent">

            <Button
                android:id="@+id/button107"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.5"
                android:visibility="invisible" />

            <ImageView
                android:id="@+id/imageView22"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/excexc"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:contentDescription="@string/todo" />

            <Button
                android:id="@+id/button117"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.5"
                android:visibility="invisible" />
        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="10"
            tools:ignore="UselessParent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:orientation="horizontal"
                android:weightSum="10">

                <Button
                    android:id="@+id/button162"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:text="@string/button"
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/imageView23"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="8"
                    android:adjustViewBounds="true"
                    app:srcCompat="@drawable/inven"
                    android:contentDescription="@string/todo" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_weight="7.8"
                android:layout_marginHorizontal="10dp">

                <TableLayout
                    android:id="@+id/table8"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    tools:ignore="UselessParent">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="4"
                        android:background="#555555"
                        tools:ignore="UselessParent">

                        <Button
                            android:id="@+id/button163"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle"
                            android:layout_marginLeft="7dp" />

                        <Button
                            android:id="@+id/button164"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle"
                            android:layout_marginLeft="7dp" />

                        <Button
                            android:id="@+id/button165"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle"
                            android:layout_marginLeft="7dp" />

                        <Button
                            android:id="@+id/button166"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle"
                            android:layout_marginLeft="7dp" />

                        <Button
                            android:id="@+id/button167"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginLeft="7dp"
                            android:layout_marginRight="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle" />
                    </TableRow>

                </TableLayout>

            </LinearLayout>

            <Button
                android:id="@+id/button168"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="2.2"
                android:visibility="invisible" />

        </LinearLayout>

    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>