<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NextActivityLawn">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/button106"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/lawn" />

    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/rotc"
            android:contentDescription="@string/rotc_image" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:ignore="UselessParent"
            android:weightSum="10">

            <Button
                android:id="@+id/button108"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:visibility="invisible"
                tools:visibility="invisible"
                android:layout_weight="2.4"/>

            <TextView
                android:id="@+id/textView26"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="7.6"
                android:background="@drawable/talk"
                android:fontFamily="@font/bmjua"
                android:paddingLeft="20dp"
                android:paddingTop="15dp"
                android:paddingRight="20dp"
                android:text="@string/rotc_text"
                android:textColor="#000000"
                app:autoSizeMaxTextSize="28dp"
                app:autoSizeMinTextSize="12dp"
                app:autoSizeStepGranularity="2dp"
                app:autoSizeTextType="uniform"
                tools:ignore="InefficientWeight" />

        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:weightSum="4"
            tools:ignore="UselessParent">

            <Button
                android:id="@+id/button110"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:visibility="invisible"
                tools:visibility="invisible"
                tools:ignore="ButtonStyle" />

            <Button
                android:id="@+id/button111"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_marginHorizontal="5dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="1"
                android:background="@drawable/button_radius"
                android:fontFamily="@font/bmjua"
                android:text="@string/out"
                android:textColor="#000000"
                app:autoSizeMaxTextSize="26dp"
                app:autoSizeMinTextSize="12dp"
                app:autoSizeStepGranularity="2dp"
                app:autoSizeTextType="uniform"
                tools:ignore="ButtonStyle" />

            <Button
                android:id="@+id/button112"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_marginHorizontal="5dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="1"
                android:background="@drawable/button_radius"
                android:fontFamily="@font/bmjua"
                android:text="@string/next"
                android:textColor="#000000"
                app:autoSizeMaxTextSize="26dp"
                app:autoSizeMinTextSize="12dp"
                app:autoSizeStepGranularity="2dp"
                app:autoSizeTextType="uniform"
                tools:ignore="ButtonStyle" />

            <Button
                android:id="@+id/button113"
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:visibility="invisible"
                tools:visibility="invisible"
                tools:ignore="ButtonStyle" />
        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:weightSum="3"
            tools:ignore="UselessParent">

            <Button
                android:id="@+id/button114"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:visibility="invisible"
                tools:visibility="invisible"
                tools:ignore="ButtonStyle" />

            <Button
                android:id="@+id/button115"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_marginHorizontal="5dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="1"
                android:background="@drawable/button_radius"
                android:fontFamily="@font/bmjua"
                android:text="@string/open_draw"
                android:textColor="#000000"
                app:autoSizeMaxTextSize="26dp"
                app:autoSizeMinTextSize="12dp"
                app:autoSizeStepGranularity="2dp"
                app:autoSizeTextType="uniform"
                tools:ignore="ButtonStyle" />

            <Button
                android:id="@+id/button116"
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:visibility="invisible"
                tools:visibility="invisible"
                tools:ignore="ButtonStyle" />
        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="10"
            tools:ignore="UselessParent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:orientation="horizontal"
                android:weightSum="10">

                <Button
                    android:id="@+id/button119"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:text=""
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/imageView17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="8"
                    android:adjustViewBounds="true"
                    app:srcCompat="@drawable/inven"
                    android:contentDescription="@string/inven" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_weight="7.8"
                android:layout_marginHorizontal="10dp">

                <TableLayout
                    android:id="@+id/table3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="4"
                        android:background="#555555">

                        <Button
                            android:id="@+id/button128"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle" />

                        <Button
                            android:id="@+id/button129"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle" />

                        <Button
                            android:id="@+id/button130"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle" />

                        <Button
                            android:id="@+id/button131"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle" />

                        <Button
                            android:id="@+id/button132"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginLeft="7dp"
                            android:layout_marginRight="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle" />
                    </TableRow>

                </TableLayout>

            </LinearLayout>

            <Button
                android:id="@+id/button133"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="2.2"
                android:visibility="invisible" />

        </LinearLayout>

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>