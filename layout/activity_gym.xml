<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/button38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/gym" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="10"
            tools:ignore="UselessParent">

            <Button
                android:id="@+id/button49"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="4"
                android:visibility="invisible"
                tools:visibility="invisible" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginHorizontal="20dp"
                android:layout_weight="3"
                android:orientation="horizontal"
                android:weightSum="2">


                <Button
                    android:id="@+id/button51"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_radius"
                    android:fontFamily="@font/bmjua"
                    android:text="@string/exercise2"
                    android:textColor="#000000"
                    android:textSize="50sp"
                    tools:ignore="InefficientWeight,NestedWeights" />

            </LinearLayout>

            <Button
                android:id="@+id/button50"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2"
                android:visibility="invisible"
                tools:visibility="invisible" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/talk"
                android:fontFamily="@font/bmjua"
                android:paddingLeft="20dp"
                android:paddingTop="30dp"
                android:paddingRight="20dp"
                android:text="@string/ss_gym"
                android:textColor="#000000"
                app:autoSizeMaxTextSize="28dp"
                app:autoSizeMinTextSize="12dp"
                app:autoSizeStepGranularity="2dp"
                app:autoSizeTextType="uniform" />

        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginBottom="20dp"
            android:layout_marginEnd="20dp"
            tools:ignore="UselessParent"
            android:layout_marginRight="20dp">

            <Button
                android:id="@+id/button44"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:visibility="invisible"
                tools:visibility="invisible"
                tools:ignore="ButtonStyle" />

            <Button
                android:id="@+id/button45"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_marginHorizontal="5dp"
                android:layout_weight="3"
                android:background="@drawable/button_radius"
                android:fontFamily="@font/bmjua"
                android:text="@string/exit"
                android:textColor="#000000"
                app:autoSizeMaxTextSize="26dp"
                app:autoSizeMinTextSize="12dp"
                app:autoSizeStepGranularity="2dp"
                app:autoSizeTextType="uniform"
                tools:ignore="ButtonStyle" />

            <Button
                android:id="@+id/button43"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_marginHorizontal="5dp"
                android:layout_weight="3"
                android:background="@drawable/button_radius"
                android:fontFamily="@font/bmjua"
                android:text="@string/next"
                android:textColor="#000000"
                app:autoSizeMaxTextSize="26dp"
                app:autoSizeMinTextSize="12dp"
                app:autoSizeStepGranularity="2dp"
                app:autoSizeTextType="uniform"
                tools:ignore="ButtonStyle" />

        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:ignore="UselessParent">

            <Button
                android:id="@+id/button46"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="invisible"
                tools:visibility="invisible"
                android:layout_weight="1"
                />

            <Button
                android:id="@+id/button47"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text=""
                android:fontFamily="@font/bmjua"
                android:gravity="center"
                android:textSize="40sp"
                android:textColor="#000000"
                android:background="@color/colortextbackground2"
                />

            <Button
                android:id="@+id/button48"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="invisible"
                tools:visibility="invisible"
                android:layout_weight="1"
                />
        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="10"
            tools:ignore="UselessParent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:orientation="horizontal"
                android:weightSum="10">

                <Button
                    android:id="@+id/button161"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:text="@string/button"
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/imageView21"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="8"
                    android:adjustViewBounds="true"
                    app:srcCompat="@drawable/inven"
                    android:contentDescription="@string/todo" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_weight="7.8"
                android:layout_marginHorizontal="10dp">

                <TableLayout
                    android:id="@+id/table7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="4"
                        android:background="#555555">

                        <Button
                            android:id="@+id/button155"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle"
                            android:layout_marginLeft="7dp" />

                        <Button
                            android:id="@+id/button156"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle"
                            android:layout_marginLeft="7dp" />

                        <Button
                            android:id="@+id/button157"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle"
                            android:layout_marginLeft="7dp" />

                        <Button
                            android:id="@+id/button158"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginStart="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle"
                            android:layout_marginLeft="7dp" />

                        <Button
                            android:id="@+id/button159"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginVertical="7dp"
                            android:layout_marginLeft="7dp"
                            android:layout_marginRight="7dp"
                            android:layout_weight="1"
                            android:background="@drawable/tile"
                            android:fontFamily="@font/bmjua"
                            android:gravity="end|bottom"
                            android:text=""
                            android:textColor="#000000"
                            app:autoSizeMaxTextSize="25dp"
                            app:autoSizeMinTextSize="10dp"
                            app:autoSizeStepGranularity="2dp"
                            app:autoSizeTextType="uniform"
                            tools:ignore="ButtonStyle" />
                    </TableRow>

                </TableLayout>

            </LinearLayout>

            <Button
                android:id="@+id/button160"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="2.2"
                android:visibility="invisible" />

        </LinearLayout>

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>