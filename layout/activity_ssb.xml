<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/button17"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/sunsun" />

    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:weightSum="10">

        <Button
            android:id="@+id/button16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:visibility="invisible"
            tools:visibility="invisible" />


        <TableLayout
            android:id="@+id/table"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:background="#D6C7ED"
            android:stretchColumns="*"
            android:weightSum="4">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bmjua"
                    android:gravity="center"
                    android:text="@string/stat"
                    android:textColor="#000000"
                    android:textSize="25sp"/>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bmjua"
                    android:layout_gravity="center"
                    android:text="@string/know"
                    android:textSize="20sp"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bmjua"
                    android:gravity="center"
                    android:text=""
                    android:textSize="20sp"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bmjua"
                    android:layout_gravity="center"
                    android:text="@string/health"
                    android:textSize="20sp"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bmjua"
                    android:gravity="center"
                    android:text=""
                    android:textSize="20sp"
                    android:textColor="#000000" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bmjua"
                    android:gravity="center"
                    android:text="@string/word"
                    android:textSize="20sp"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bmjua"
                    android:gravity="center"
                    android:text=""
                    android:textSize="20sp"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bmjua"
                    android:gravity="center"
                    android:text="@string/full"
                    android:textSize="20sp"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bmjua"
                    android:gravity="center"
                    android:text=""
                    android:textSize="20sp"
                    android:textColor="#000000" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bmjua"
                    android:gravity="center"
                    android:text="@string/love"
                    android:textSize="20sp"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bmjua"
                    android:gravity="center"
                    android:text=""
                    android:textSize="20sp"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bmjua"
                    android:gravity="center"
                    android:text=""
                    android:textSize="20sp"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bmjua"
                    android:gravity="center"
                    android:text=""
                    android:textSize="20sp"
                    android:textColor="#000000" />
            </TableRow>
        </TableLayout>

        <Button
            android:id="@+id/button19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:visibility="invisible"
            tools:visibility="invisible" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_weight="1"
            android:background="@drawable/talk"
            android:fontFamily="@font/bmjua"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="15dp"
            android:text="@string/ssb_text"
            android:textColor="#000000"
            app:autoSizeMaxTextSize="28dp"
            app:autoSizeMinTextSize="12dp"
            app:autoSizeStepGranularity="2dp"
            app:autoSizeTextType="uniform" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:weightSum="4"
        android:layout_margin="15dp">

        <Button
            android:id="@+id/button22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:visibility="invisible"
            tools:visibility="invisible"
            tools:ignore="ButtonStyle" />

        <Button
            android:id="@+id/button20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:fontFamily="@font/bmjua"
            android:text="@string/stat_look"
            android:background="@drawable/button_radius"
            android:textColor="#000000"
            app:autoSizeMaxTextSize="24dp"
            app:autoSizeMinTextSize="12dp"
            app:autoSizeStepGranularity="2dp"
            app:autoSizeTextType="uniform"
            android:layout_weight="1"
            android:layout_marginHorizontal="5dp"
            tools:ignore="ButtonStyle" />

        <Button
            android:id="@+id/button21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:text="@string/out"
            android:background="@drawable/button_radius"
            android:textColor="#000000"
            android:fontFamily="@font/bmjua"
            app:autoSizeMaxTextSize="24dp"
            app:autoSizeMinTextSize="12dp"
            app:autoSizeStepGranularity="2dp"
            app:autoSizeTextType="uniform"
            android:layout_weight="1"
            android:layout_marginHorizontal="5dp"
            tools:ignore="ButtonStyle" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:weightSum="10">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:orientation="horizontal"
            android:weightSum="10">

            <Button
                android:id="@+id/button118"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text=""
                android:visibility="invisible" />

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="8"
                android:adjustViewBounds="true"
                app:srcCompat="@drawable/inven"
                android:contentDescription="@string/bag_image"/>

        </LinearLayout>

        <TableLayout
            android:id="@+id/table2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="7.8"
            android:layout_marginHorizontal="10dp">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:background="#555555"
                tools:ignore="UselessParent">

                <Button
                    android:id="@+id/button121"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginVertical="7dp"
                    android:layout_marginEnd="3.5dp"
                    android:layout_marginStart="7dp"
                    android:layout_weight="1"
                    android:background="@drawable/tile"
                    android:fontFamily="@font/bmjua"
                    android:gravity="end|bottom"
                    android:text=""
                    android:textColor="#000000"
                    app:autoSizeMaxTextSize="25dp"
                    app:autoSizeMinTextSize="10dp"
                    app:autoSizeStepGranularity="2dp"
                    app:autoSizeTextType="uniform"
                    tools:ignore="ButtonStyle" />

                <Button
                    android:id="@+id/button122"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginVertical="7dp"
                    android:layout_marginRight="3.5dp"
                    android:layout_marginLeft="3.5dp"
                    android:layout_weight="1"
                    android:background="@drawable/tile"
                    android:fontFamily="@font/bmjua"
                    android:gravity="end|bottom"
                    android:text=""
                    android:textColor="#000000"
                    app:autoSizeMaxTextSize="25dp"
                    app:autoSizeMinTextSize="10dp"
                    app:autoSizeStepGranularity="2dp"
                    app:autoSizeTextType="uniform"
                    tools:ignore="ButtonStyle" />

                <Button
                    android:id="@+id/button123"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginVertical="7dp"
                    android:layout_marginRight="3.5dp"
                    android:layout_marginLeft="3.5dp"
                    android:layout_weight="1"
                    android:background="@drawable/tile"
                    android:fontFamily="@font/bmjua"
                    android:gravity="end|bottom"
                    android:text=""
                    android:textColor="#000000"
                    app:autoSizeMaxTextSize="25dp"
                    app:autoSizeMinTextSize="10dp"
                    app:autoSizeStepGranularity="2dp"
                    app:autoSizeTextType="uniform"
                    tools:ignore="ButtonStyle" />

                <Button
                    android:id="@+id/button124"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginVertical="7dp"
                    android:layout_marginRight="3.5dp"
                    android:layout_marginLeft="3.5dp"
                    android:layout_weight="1"
                    android:background="@drawable/tile"
                    android:fontFamily="@font/bmjua"
                    android:gravity="end|bottom"
                    android:text=""
                    android:textColor="#000000"
                    app:autoSizeMaxTextSize="25dp"
                    app:autoSizeMinTextSize="10dp"
                    app:autoSizeStepGranularity="2dp"
                    app:autoSizeTextType="uniform"
                    tools:ignore="ButtonStyle" />

                <Button
                    android:id="@+id/button127"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginVertical="7dp"
                    android:layout_marginStart="3.5dp"
                    android:layout_marginEnd="7dp"
                    android:layout_weight="1"
                    android:background="@drawable/tile"
                    android:fontFamily="@font/bmjua"
                    android:gravity="end|bottom"
                    android:text=""
                    android:textColor="#000000"
                    app:autoSizeMaxTextSize="25dp"
                    app:autoSizeMinTextSize="10dp"
                    app:autoSizeStepGranularity="2dp"
                    app:autoSizeTextType="uniform"
                    tools:ignore="ButtonStyle" />
            </TableRow>

        </TableLayout>
        <Button
            android:id="@+id/button126"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="2.2"
            android:visibility="invisible" />
    </LinearLayout>



    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/button174"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="10dp"
            android:text="@string/time_see"
            android:background="@drawable/button_radius"
            android:textColor="#000000"
            android:fontFamily="@font/bmjua"
            app:autoSizeMaxTextSize="22dp"
            app:autoSizeMinTextSize="12dp"
            app:autoSizeStepGranularity="2dp"
            app:autoSizeTextType="uniform" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView29"
            android:layout_width="wrap_content"
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/button_radius"
            android:fontFamily="@font/bmjua"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:text="@string/time_t"
            android:textColor="#000000"
            android:layout_height="60dp"
            android:paddingTop="7dp"
            android:textSize="40sp" />
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>